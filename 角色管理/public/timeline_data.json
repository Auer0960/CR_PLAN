    {
      "id": "EV4",
      "title": "血月之夜",
      "startYear": -9,
      "endYear": -9,
      "isContinuous": false,
      "size": "large",
      "parentEventIds": [],
      "relatedEventIds": [],
      "characterIds": [
        "8635cd035e37",
